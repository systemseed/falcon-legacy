  "scripts": {
    "watch:css": "npm run build:css && node-sass src/styles/sass -o src/styles/css/ --output-style compressed --watch --recursive",
    "start:js": "react-scripts start",
    "start": "npm-run-all -p watch:css start:js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "start:server": "NODE_ENV=development node server/index.js",
    "start:server:prod": "NODE_ENV=production node server/index.js",
    "build:css": "node-sass src/styles/sass -o src/styles/css/ --output-style compressed",
    "build:snippets": "node build_scripts/build-snippets.js",
    "build:gzip": "gzip -k build/static/js/*.js && gzip -k build/static/css/*.css",
    "build:platform": "npm run build:css && react-scripts build && npm run build:gzip && mv build build-platform && mkdir build",
    "build": "npm run build:css && react-scripts build && npm run build:snippets"
  }
